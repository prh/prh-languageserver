    "@types/vscode": "^1.40.0",
    "conventional-changelog-cli": "^1.3.2",
    "diff": "^4.0.1",
    "mocha": "^3.5.0",
    "ts-node": "^8.5.4",
    "typescript": "3.6.3",
    "typescript-formatter": "^6.0.0",
    "vscode-jsonrpc": "4.0.0",
    "vscode-languageclient": "5.2.1"
